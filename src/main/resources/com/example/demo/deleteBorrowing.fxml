<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.DeleteBorrowingController">
   <children>
      <TableView fx:id="borrowingTableView" layoutY="30.0" prefHeight="370.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="userNameColumn" prefWidth="193.0" text="Username" />
          <TableColumn fx:id="bookNameColumn" prefWidth="238.0" text="Book Borrowed" />
            <TableColumn fx:id="cancelButtonColumn" prefWidth="168.0" text="Cancel Borrowing" />
        </columns>
      </TableView>
      <Button fx:id="returnButton" layoutX="258.0" layoutY="2.0" mnemonicParsing="false" onAction="#returnButtonClicked" text="Return" />
   </children>
</AnchorPane>
